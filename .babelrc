{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@pages": "../pages",
          "@styles": "../styles",
          "@hooks": "./src/hooks",
          "@components": "./src/components",
          "@elements": "./src/elements",
          "@typing": "./src/typing",
          "@queries": "./src/queries",
          "@constant": "./src/constant"
        }
      }
    ],
    ["@emotion/babel-plugin"]
  ]
}
